{
  "description": "Import Ingest Pipeline for puppetserver.log",
  "input_method": "stdin",
  "parameters": {
    "elastic_host": {
      "description": "The Elasticsearch API host",
      "type": "String[1]",
      "default": "localhost"
    },
    "elastic_port": {
      "description": "The Elasticsearch API port",
      "type": "Integer",
      "default": 9200
    },
    "protocol": {
      "description": "Protocol to use.",
      "default": "http",
      "type": "String[1]"
    },
    "pipeline_name":{
      "description": "Name of the Ingest Pipeline",
      "default": "puppetserver_log",
      "type": "String[1]"
    }
  }
}
